EMBED %WindowManagerMethodCodeSection
[INSTANCES]
WHEN 'TakeCompleted'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4500
PROPERTY:END
IF MCP:FechaContable_date < GLO:FECHAINICIAL or MCP:FechaContable_date > GLO:FECHAFINAL
   case message('La Fecha Contable No Corresponde al Período seleccionado.'&chr(13)&chr(10)&|
                '    Quiere Continuar con el nuevo periodo: '&year(MCP:FechaContable_date)&'-'&month(MCP:FechaContable_date)&' ?','Fecha Contable',icon:question,Button:yes+Button:No,Button:No,1)
   of Button:yes
      clear(wdum:record)
      wdum{prop:sql} = 'SELECT CERRADO, ANO FROM PRI WHERE ANO='&FnSqlFmt(year(MCP:FechaContable_date))&' AND MES='&FnSqlFmt(Month(MCP:FechaContable_date))
      access:wdum.next()
      if wdu:c2 = ''
         Message('El Periodo Contable '&year(MCP:FechaContable_date)&'-'&month(MCP:FechaContable_date)&' No Existe','Fecha Contable',icon:hand)
         Cycle
      end
      if wdu:c1 = 1
         Message('El Periodo Contable '&year(MCP:FechaContable_date)&'-'&month(MCP:FechaContable_date)&' está Cerrado','Fecha Contable',icon:hand)
         Cycle
      end
      mcp:ano = year(MCP:FechaContable_date)
      mcp:mes = Month(MCP:FechaContable_date)
   of Button:NO
      select(?MCP:FECHACONTABLE_DATE)
      Cycle
   END
END

IF FnValorVariable('IDCON_TCOM_ECAJ')=MCP:COMPROBANTE
   MCP:RPT_COMPROBANTE='EGRESO'
END

CASE THISWINDOW.REQUEST
OF INSERTRECORD
   BIND('SALIDA', MCP:NROCOMPROBANTE)
   clear(wdum:record)
   wdum{prop:sql}='CALL SPQ_GENSEQUENCE ('&FnSqlFmt(GLO:IDSEDE)&',''@MCP'', '&FnSqlFmt(GLO:IDSEDE)&', 14, ''NVO'', &SALIDA[OUT])'
   IF ERRORCODE() THEN
      SETCLIPBOARD(wdum{prop:sql})
      MESSAGE('Error: ' & fileerror(), 'Error Krystalos', ICON:HAND)
   END
   IF MCP:COMPROBANTE<>FnValorVariable('IDCON_TCOM_FTR')
      MCP:NOREFERENCIA=Format(FnGeneraConsecutivo(GLO:IDSEDE,'@COM'&Clip(MCP:COMPROBANTE)),@n08)
   END
   
OF CHANGERECORD
   CLEAR(WDF:RECORD)
   WDF{PROP:SQL}='SELECT DBO.FNK_FECHA_SIN_MLS(GETDATE())'
   ACCESS:WDF.NEXT()
   MCP:Usuario = glo:usuario
   MCP:FECHA_DATE = WDF:C1_DATE
   MCP:FECHA_TIME = WDF:C1_TIME
   IF LOC:COMPROBANTE_ANT<>MCP:COMPROBANTE
      IF MCP:COMPROBANTE<>FnValorVariable('IDCON_TCOM_FTR')
         MCP:NOREFERENCIA=Format(FnGeneraConsecutivo(GLO:IDSEDE,'@COM'&Clip(MCP:COMPROBANTE)),@n08)
         Message('*** Atencion: El No. de Comprobante ha Cambiado por '&MCP:COMPROBANTE&' '&MCP:NOREFERENCIA,'Comprobante',icon:asterisk)
      END
   END
END

[END]
[END]
WHEN 'Init'
[INSTANCES]
WHEN '(),BYTE'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 8125
PROPERTY:END
case ThisWindow.Request
of ChangeRecord orof DeleteRecord
   glo:ano = mcp:ano
   glo:mes = mcp:mes
end

CLEAR(PRI:RECORD)
PRI:COMPANIA=GLO:COMPANIA
PRI:ANO=GLO:ANO
PRI:MES=GLO:MES
IF ACCESS:PRI.TRYFETCH(PRI:PRICIAANOMES)
   Disable(?CurrentTab)
   Disable(?OK)
   Message('Periodo Contable No Existe, No se pueden hacer Actualizaciones.','Periodo',icon:hand)
ELSE
 glo:fechainicial = pri:fecha_ini_date
 glo:fechafinal   = pri:fecha_fin_date
 IF PRI:CERRADO=0
    CASE THISWINDOW.REQUEST
    OF INSERTRECORD
       CLEAR(WDF:RECORD)
       WDF{PROP:SQL}='SELECT DBO.FNK_FECHA_SIN_MLS(GETDATE())'
       ACCESS:WDF.NEXT()
       MCP:COMPANIA=GLO:COMPANIA
       MCP:Usuario = glo:usuario
       MCP:FECHACONTABLE_DATE = WDF:C1_DATE
       MCP:FECHACONTABLE_TIME = WDF:C1_TIME
       MCP:FECHA_DATE = WDF:C1_DATE
       MCP:FECHA_TIME = WDF:C1_TIME
       MCP:ANO=GLO:ANO
       MCP:MES=GLO:MES
       MCP:TOTALDEBITO=0
       MCP:TOTALCREDITO=0
       MCP:PROCEDENCIA='MANUAL'
       MCP:IDSEDE=GLO:IDSEDE
       MCP:ESTADO=0
    OF CHANGERECORD
       IF MCP:PROCEDENCIA <> 'MANUAL'
          Disable(?CurrentTab)
          Disable(?OK)
          dISABLE(?MCP:ANULADO)
          DISABLE(?OK)
       ELSE
          LOC:COMPROBANTE_ANT=MCP:COMPROBANTE
          !Disable(?CallLookup:COM)
          !?MCP:COMPROBANTE{PROP:READONLY}=True
          CASE MCP:ESTADO
          OF 0
             IF MCP:PROCEDENCIA<>'MANUAL'
                Disable(?MCP:NOREFERENCIA)
             END
          OF 1
             IF MCP:PROCEDENCIA<>'MANUAL'
                Disable(?MCP:NOREFERENCIA)
             END
          OF 2
             Disable(?CurrentTab)
             Disable(?OK)
          END
       END
    OF DELETERECORD
       IF MCP:PROCEDENCIA <> 'MANUAL'
          Disable(?CurrentTab)
          Disable(?OK)
          dISABLE(?MCP:ANULADO)
          DISABLE(?OK)
       ELSE
          Disable(?CurrentTab)
          IF MCP:PROCEDENCIA='MANUAL' OR MCP:PROCEDENCIA='MCA'
             CASE MCP:ESTADO
             OF 2
                Disable(?OK)
             END
          ELSE
             Disable(?OK)
          END
       END
    END

  IF MCP:COMPROBANTE=FnValorVariable('IDCON_TCOM_FTR')
   ?MCP:NOREFERENCIA{PROP:REQ}=True
   IF MCP:PROCEDENCIA='MANUAL' OR MCP:PROCEDENCIA='MCA'
    Enable(?MCP:NOREFERENCIA)
   END
  ELSE
   ?MCP:NOREFERENCIA{PROP:REQ}=False
   Disable(?MCP:NOREFERENCIA)
  END
 ELSE
  Disable(?CurrentTab)
  Disable(?OK)
  Message('Periodo Contable Cerrado, No se pueden hacer Actualizaciones.','Periodo',icon:hand)

 END
END

CLEAR(COM:RECORD)
COM:COMPANIA=GLO:COMPANIA
COM:COMPROBANTE=MCP:COMPROBANTE
IF ACCESS:COM.TRYFETCH(COM:COMCOMPROBANTE) THEN CLEAR(COM:RECORD).

CLEAR(TER:RECORD)
TER:IDTERCERO=MCP:IDTERCERO
IF ACCESS:TER.TRYFETCH(TER:TERIDTERCERO) THEN CLEAR(TER:RECORD).

IF FnValorVariable('IDCON_TCOM_ECAJ')= MCP:COMPROBANTE
 UnHide(?Group:Cheque)
ELSE
 Hide(?Group:Cheque)
END

DISPLAY

[END]
[END]
