PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
ValidaComprobante Routine
 IF FnValorVariable('IDCON_TCOM_ECAJ')=MCP:COMPROBANTE
  UnHide(?Group:Cheque)
 ELSE
  Hide(?Group:Cheque)
 END

 IF UPPER(FnValorVariable('PERMITIR_TODO'))='SI'
  if upper(FnValorVariable('IDTIPOCXP_DEFAULT'))='ARS'
   !?MCP:NOREFERENCIA{PROP:REQ}=True
   !Enable(?MCP:NOREFERENCIA)
   Enable(?MCP:NOCHEQUE)
   Enable(?MCP:VALOR_CHEQUE)
  else
   IF MCP:COMPROBANTE=FnValorVariable('IDCON_TCOM_FTR')
    ?MCP:NOREFERENCIA{PROP:REQ}=True
    Enable(?MCP:NOREFERENCIA)
   ELSE
    ?MCP:NOREFERENCIA{PROP:REQ}=False
    Disable(?MCP:NOREFERENCIA)
   END
  end
 ELSE
  IF MCP:COMPROBANTE=FnValorVariable('IDCON_TCOM_FTR')
   ?MCP:NOREFERENCIA{PROP:REQ}=True
   Enable(?MCP:NOREFERENCIA)
  ELSE
   ?MCP:NOREFERENCIA{PROP:REQ}=False
   Disable(?MCP:NOREFERENCIA)
  END
  Disable(?MCP:NOCHEQUE)
  Disable(?MCP:VALOR_CHEQUE)
 END
