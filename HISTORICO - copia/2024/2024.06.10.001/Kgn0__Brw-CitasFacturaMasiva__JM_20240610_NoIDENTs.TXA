EMBED %ControlEventHandling
[INSTANCES]
WHEN '?BMarcar'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
SETCURSOR(CURSOR:WAIT)
CIT:MARCAFAC = 1 - CIT:MARCAFAC
IF CIT:MARCAFAC = 1
   CIT:CNSFACT  = CNSF
ELSE
   SETNULL(CIT:CNSFACT)
END
ACCESS:CIT.UPDATE()
BRW1.RESETFROMFILE
!ThisWindow.Reset(1)
POST(Event:ScrollDown,?Browse:1)
Select(?Browse:1)
SETCURSOR(CURSOR:ARROW)
!do CONTAR
[END]
[END]
WHEN '?BMarcaT'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
 LOC:SQL='UPDATE CIT SET MARCAFAC = 1, CNSFACT = '&FNSQLFMT(CNSF)&CHR(13)&|
         'FROM CIT A INNER JOIN SER B ON A.IDSERVICIO=B.IDSERVICIO INNER JOIN AFI C ON A.IDAFILIADO=C.IDAFILIADO '&chr(13)&|
         'WHERE ISNULL(MARCAFAC,0)=0 AND '&loc:filtro
 IF FnValorVariable('IDDEPURAR')='SI'
    SETCLIPBOARD(LOC:SQL)
    STOP('Marcar todo')
 END
 clear(wdum:record)
 wdum{prop:sql}=LOC:SQL
 BRW1.RESETFROMFILE
 !do CONTAR
 ThisWindow.Reset(1)

[END]
[END]
WHEN '?BDmarcat'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
 LOC:SQL='UPDATE CIT SET MARCAFAC = 0, CNSFACT = NULL '&CHR(13)&|
         'FROM CIT A INNER JOIN SER B ON A.IDSERVICIO=B.IDSERVICIO INNER JOIN AFI C ON A.IDAFILIADO=C.IDAFILIADO '&chr(13)&|
         'WHERE MARCAFAC = 1 AND '&loc:filtro
 IF FnValorVariable('IDDEPURAR')='SI'
    SETCLIPBOARD(LOC:SQL)
    STOP('Desmarcar todo')
 END
 clear(wdum:record)
 wdum{prop:sql}=LOC:SQL
 BRW1.RESETFROMFILE
 ThisWindow.Reset(1)
 !do CONTAR

[END]
[END]
WHEN '?BAsignar'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
LOC:MARCA=1
ThisWindow.Reset(1)
POST(EVENT:ScrollTop,?Browse:1)
Select(?Browse:1)
SET(BRW1.View)
LOOP
   NEXT(BRW1.View)
   IF ERRORCODE() THEN BREAK.
   BRW1.SETQUEUERECORD
   LOC:sQL='INSERT INTO FMASD(CNSFMAS,NOADMISION,VR_TOTAL,FACTURADA,VLR_IVA)'&CHR(13)&|
      ' SELECT '&FnSQLFmt(CNSF)&', '&FnSQLFmt(CIT:CONSECUTIVO)&','&CIT:VALORTOTAL&', 0, '&CIT:VALORCOPAGO
   CLEAR(WDUM:RECORD)
   WDUM{PROP:SQL}=LOC:SQL
END
POST(EVENT:CloseWindow)
[END]
[END]
WHEN '?BAutorizacion'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
 SETCURSOR(CURSOR:WAIT)
 LOC:SQL='UPDATE CIT SET NOAUTORIZACION='&FnSQLFmt(LOC:AUTORIZACION)&CHR(13)&|
         'FROM CIT A INNER JOIN SER B ON A.IDSERVICIO=B.IDSERVICIO INNER JOIN AFI C ON A.IDAFILIADO=C.IDAFILIADO '&chr(13)&|
         'WHERE MARCAFAC = 1 AND CNSFACT = '&FNSQLFMT(CNSF)&' AND '&loc:filtro
 IF FnValorVariable('IDDEPURAR')='SI'
    SETCLIPBOARD(LOC:SQL)
    STOP('Actualizar Autorizacion')
 END
 clear(wdum:record)
 wdum{prop:sql}=LOC:SQL
 BRW1.RESETFROMFILE
 SETCURSOR(CURSOR:Arrow)
 !ThisWindow.Reset(1)


[END]
[END]
WHEN '?Browse:1'
[INSTANCES]
WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
IF LOC:MARCAR=1
   IF KEYCODE() = 1 THEN
      POST(EVENT:Accepted,?BMarcar)
   END
END
[END]
[END]
[END]
EMBED %ProcedureRoutines
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
Contar ROUTINE

 LOC:SQL='SELECT SUM(CASE WHEN MARCAFAC = 1 AND CNSFACT = '&FNSQLFMT(CNSF)&' THEN 1 ELSE 0 END),COUNT(1) '&chr(13)&|
                 ' FROM CIT A'&chr(13)&|
                  'INNER JOIN SER B ON A.IDSERVICIO=B.IDSERVICIO INNER JOIN AFI C ON A.IDAFILIADO=C.IDAFILIADO '&chr(13)&|
                  'WHERE 1=1 '
 IF LOC:FILTRO<>''
        LOC:SQL=CLIP(LOC:SQL)&' AND '&LOC:FILTRO
 END
 CLEAR(WDUM:RECORD)
 IF FnValorVariable('IDDEPURAR')='SI'
         SETCLIPBOARD(LOC:SQL)
         STOP('CONTEO')
 END
 WDUM{PROP:SQL}=LOC:SQL
 Access:WDUM.Next()

 LOC:TOTAL='Marcadas: '&WDUM:C1&' de '&WDUM:C2
[END]
