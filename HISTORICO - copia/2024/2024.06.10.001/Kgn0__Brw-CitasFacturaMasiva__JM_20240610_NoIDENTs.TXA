EMBED %BrowserMethodCodeSection
[INSTANCES]
WHEN '1'
[INSTANCES]
WHEN 'ApplyFilter'
[INSTANCES]
WHEN '()'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4500
PROPERTY:END
LOC:FILTRO=' A.IDTERCEROCA='&FnSQLFmt(IDTER)&' AND A.IDPLAN='&FnSQLFmt(IDPLAN)&' AND COALESCE(A.FACTURADA,0)=0 AND COALESCE(VALORTOTAL,0)>0 AND NOT EXISTS(SELECT * FROM FMASD WHERE FMASD.CNSFMAS='&FnSQLFmt(CNSF)&' AND NOADMISION=A.CONSECUTIVO)'

IF LOC:FECHAINI<>''
   LOC:FILTRO=CLIP(LOC:FILTRO)&' AND A.FECHA>='&FnSQLFmt(FORMAT(LOC:FECHAINI,@D06))
END
IF LOC:FECHAFIN<>''
   LOC:FILTRO=CLIP(LOC:FILTRO)&' AND A.FECHA<'&FnSQLFmt(FORMAT(LOC:fECHAFIN+1,@D06))
END
IF LOC:MARCA=1
   LOC:FILTRO=CLIP(LOC:FILTRO)&' AND A.MARCAFAC=1 AND A.CNSFACT='&FnSQLFmt(CNSF)
END
IF LOC:IDAFILIADO<>''
   LOC:FILTRO=CLIP(LOC:FILTRO)&' AND EXISTS(SELECT * FROM AFI WHERE AFI.IDAFILIADO=A.IDAFILIADO AND CONCAT(AFI.IDAFILIADO, AFI.DOCIDAFILIADO, AFI.NOMBREAFI) LIKE ''%'&LOC:IDAFILIADO&'%'')'
END
IF LOC:MEDICO<>''
   LOC:FILTRO=CLIP(LOC:FILTRO)&' AND EXISTS(SELECT * FROM MED WHERE MED.IDMEDICO=A.IDMEDICO AND CONCAT(MED.IDMEDICO, MED.NOMBRE) LIKE ''%'&LOC:MEDICO&'%'')'
END
IF LOC:SERVICIO<>''
   LOC:FILTRO=CLIP(LOC:FILTRO)&' AND EXISTS(SELECT * FROM SER WHERE SER.IDSERVICIO=A.IDSERVICIO AND CONCAT(SER.IDSERVICIO, SER.DESCSERVICIO ) LIKE ''%'&LOC:SERVICIO&'%'')'
END
IF LOC:AUTORIZACIONF<>''
   LOC:FILTRO=CLIP(LOC:FILTRO)&' AND A.NOAUTORIZACION LIKE ''%'&CLIP(LOC:AUTORIZACIONF)&'%'' '
END



BRW1::View:Browse{PROP:SQLFilter}=LOC:FILTRO
[END]
[END]
WHEN 'SetQueueRecord'
[INSTANCES]
WHEN '()'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4500
PROPERTY:END
CLEAR(A_SER:Record)
A_SER:IDSERVICIO=CIT:IDSERVICIO
IF Access:A_SER.TryFetch(A_SER:SERIDSERVICIO) THEN CLEAR(A_SER:Record).

CLEAR(A_MED:Record)
A_MED:IDMEDICO=CIT:IDMEDICO
IF Access:A_MED.TryFetch(A_MED:MEDIDMEDICO) THEN CLEAR(A_MED:Record).

CLEAR(A_AFI:Record)
A_AFI:IDAFILIADO=CIT.IDAFILIADO
IF Access:A_AFI.TryFetch(A_AFI:AFIIDAFILIADO) THEN CLEAR(A_AFI:RECORD).
[END]
[END]
[END]
