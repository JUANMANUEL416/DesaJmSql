PROPERTY:BEGIN
PRIORITY 7501
PROPERTY:END
LOC:PATHB=PATH()
LOC:CNS=0
IF in:tipo='HADM'
   FREE(COSTOS)
   CLEAR(WDUM:RECORD)
   LOC:sQL=' SELECT CONVERT(VARCHAR,MIN(XHNAT.FECHA),103),CONVERT(VARCHAR,MAX(XHNAT.FECHASAL),103),XHNAT.CCOSTO,CEN.DESCRIPCION '&CHR(13)&|
                  ' FROM (                                         '&CHR(13)&|
                  ' SELECT HNAT.NOADMISION,HNAT.NOITEM,HNAT.HABCAMA,HHAB.CCOSTO,HNAT.FECHA,'&CHR(13)&|
                  ' COALESCE((SELECT TOP 1 X.FECHA FROM HNAT X WHERE X.NOADMISION=HNAT.NOADMISION AND X.NOITEM>HNAT.NOITEM),'&CHR(13)&|
                  ' CASE HADM.CERRADA WHEN 1 THEN HADM.FECHAALTAENF WHEN 3 THEN HADM.FECHAALTAENF WHEN 2 THEN HADM.FECHAALTAMED '&CHR(13)&|
                  ' ELSE GETDATE() END) FECHASAL '&CHR(13)&|
                  ' FROM HNAT INNER JOIN HHAB ON HNAT.HABCAMA=HHAB.HABCAMA '&CHR(13)&|
                  '           INNER JOIN HADM ON HNAT.NOADMISION=HADM.NOADMISION '&CHR(13)&|
      ' WHERE HNAT.NOADMISION='&FnSQLFmt(GLO:NOADMISION)&') XHNAT INNER JOIN CEN ON XHNAT.CCOSTO=CEN.CCOSTO'&CHR(13)&|
      ' GROUP BY  XHNAT.CCOSTO,CEN.DESCRIPCION '
   IF FnValorVariable('IDDEPURAR')='SI'
      SETCLIPBOARD(LOC:SQL)
      STOP('CARGO CCOSTOS')
   END
   WDUM{PROP:SQL}=LOC:SQL
   LOOP UNTIL Access:WDUM.Next()
      MICONO=0
      MARCA= 0
      FECHAINI=WDUM:C1
      FECHAFIN=WDUM:C2
      CCOSTO=WDUM:C3
      DESCRIPCION=WDUM:C4
      ADD(COSTOS)
   END
   CLEAR(WDUM:RECORD)
   WDUM{PROP:SQL}='  SELECT IDEMEDICA,DESCRIPCION FROM MES WHERE EXISTS(SELECT * FROM HCA WHERE NOADMISION='&FnSQLFmt(GLO:NOADMISION)&' AND CLASE=''HC'' AND HCA.MED_ESPEC=MES.IDEMEDICA)'
   LOOP UNTIL ACCESS:WDUM.NEXT()
      LOC:FROMCCOSTO = CLIP(LOC:FROMCCOSTO)&CLIP(wdu:c1)&' --'&WDUM:C2&'|#'&CLIP(WDU:C1)&'|'
   END
   A# = LEN(CLIP(LOC:FROMCCOSTO))
   LOC:FROMCCOSTO = SUB(LOC:FROMCCOSTO,1,a#-1)
END
