            STOP('DOCXTPO FS')
         END
         GLO:Param  = ' '
         GLO:Show   = 1
         GLO:RetHandle =  ShellExecute(GLO:Handle,GLO:Op,GLO:File,GLO:Param,GLO:PathOpenfile,GLO:Show)
         If GLO:RetHandle = 0 Then
            Message('Error','Error',Icon:Exclamation)
         End
      END
   ELSE
      MESSAGE('Archivo No Encontrado en el Maestro Filestream'&CHR(13)&'Comuniquese con el Administrador del sistema','Documental',ICON:Hand)
   end
ELSE
   GLO:Handle = 0{PROP:Handle}
   GLO:Op     = 'Open'
   CLEAR(WDU2:RECORD)
   WDU2{PROP:SQL}='SELECT COALESCE(VALOR1,0) FROM TGEN WHERE TABLA=''DOCXTPO'' AND CAMPO=''TIPODOC'' AND CODIGO='&FNSQLFMT(DOCXTPO:TIPODOC)
