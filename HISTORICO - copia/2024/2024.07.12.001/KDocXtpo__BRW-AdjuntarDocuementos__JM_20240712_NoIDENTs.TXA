   END
   GLO:File   = CLIP(FNVALORVARIABLE('DOCXTPO_PAT'))&LOC:DOCUMENTO&'\'& CLIP(DOCXTPO:RUTA)
!ELSE
!   GLO:File   = CLIP((DOCXTPO:RUTA))
!END
   If FnValorVariable('IDDEPURAR')='SI'
           If AnyScreen:active()
                   AnyScreen:SetClipboard(GLO:File)
           Else
                   SetClipBoard(GLO:File)
                   Stop('DOCXTPO NORMAL')
           End
   End
   GLO:PathOpenfile   = PATH()
   GLO:Param  = ' '
   GLO:Show   = 1
   If AnyScreen:active()
          AnyScreen:DownloadFile(GLO:File)
   Else
          GLO:RetHandle =  ShellExecute(GLO:Handle,GLO:Op,GLO:File,GLO:Param,GLO:PathOpenfile,GLO:Show)
