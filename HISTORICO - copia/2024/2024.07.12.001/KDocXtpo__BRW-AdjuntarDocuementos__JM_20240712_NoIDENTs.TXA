   END
   SETPATH(LOC:PATHTEMP)
   CLEAR(DOCS:Record)
   DOCS:DocumentoID=DOCXTPO:DOCUMENTOID
   IF Access:DOCS.TryFetch(DOCS:DOCSDOCUMENTOID)=Level:Benign
          IF EXISTS(LOC:PATHTEMP&'\'&DOCS:DocNombre)
                REMOVE(LOC:PATHTEMP&'\'&DOCS:DocNombre)
          END

      IF NOT EXISTS(LOC:PATHTEMP&'\'&DOCS:DocNombre)
         loc:archivo=BLOBToFile(DOCS:DocFS,DOCS:DocNombre)
      END
      LOOP
         IF EXISTS(LOC:PATHTEMP&'\'&DOCS:DocNombre)
            BREAK
         ELSE
            I# =+1
            IF I# >=60000
               BREAK
            END
         END
      END
      IF NOT EXISTS(LOC:PATHTEMP&'\'&DOCS:DocNombre)
         MESSAGE('Archivo no Encontrado...','Documental',ICON:Clarion)
         CYCLE
      END
      loc:archivo=DOCS:DocNombre
      IF AnyScreen:Active()
         AnyScreen:DownloadFile(LOC:PATHTEMP&'\'&CLIP(DOCS:DocNombre))
      ELSE
         GLO:Handle = 0{PROP:Handle}
         GLO:PathOpenfile   = LOC:PATHTEMP
         GLO:Op     = 'Open'
         GLO:File   = LOC:PATHTEMP&'\'&DOCS:DocNombre
