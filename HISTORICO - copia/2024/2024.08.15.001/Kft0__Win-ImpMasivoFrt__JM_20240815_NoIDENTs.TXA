[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
IF GLO:TXTARCHGEN=''
   CYCLE
END
DISABLE(?BProcesar)
Relate:ARCHGEN.Open
CLEAR(ARCHGEN:record,-1)
SET(ARCHGEN)
FREE(QERROR)
I#=1
IF GLO:CNSRPDX=''
   GLO:CNSRPDX=CLIP(GLO:IDSEDE)&FORMAT(FNGENERACONSECUTIVO(GLO:IDSEDE,'@RPDX'),@N08)
ELSE
   CLEAR(WDU5:Record)
   WDU5{PROP:SQL}='DELETE RPDX WHERE CNS='&FnSQLfmt(GLO:CNSRPDX)
END
LOOP UNTIL ACCESS:ARCHGEN.NEXT()
   LOC:N_FACTURAN  = Column_Text (ARCHGEN:LINE,',',0)
   LOC:MGE='Procesando Factura Nro.:'&LOC:N_FACTURAN&' item='&I#
   DISPLAY
   CLEAR(WDU5:RECORD)
   WDU5{PROP:SQL}='SELECT COUNT(*) FROM FTR WHERE N_FACTURA='&FNSQLFMT(LOC:N_FACTURAN)
   ACCESS:WDU5.NEXT()
   IF WDU5:C1<=0
      EFACTURA = LOC:N_FACTURAN
      ITEMS   = I#
      EMENSAGE = 'Factura No encontrada .. No se puede Imprimir'
      ADD(QERROR,+EFACTURA)
      CYCLE
   END
   I#+=1
   CLEAR(RPDX:RECORD)
   RPDX:CNS  = GLO:CNSRPDX
   RPDX:ID1 = LOC:N_FACTURAN
   Access:RPDX.Insert()
END
IF RECORDS(QERROR)>0
   A.Init(QuickWindow,?SMExcel,1,1,1)
   A.CreateFile()
   A.Select('C3')
   A.Assign('Facturas Con Inconsistencias')
   A.SetAlignment(10)
   I#=6
   A.Assign(I#, 3, 'N_FACTURA')
   A.setbold(true)
   A.Assign(I#, 4, 'ITEM')
   A.setbold(true)
   I# +=1
   LOOP J#=1 TO RECORDS(QERROR)
      Get(QERROR , J#)
      A.Assign(I#, 3,  EFACTURA)
      A.Assign(I#, 4,  ITEMS)
      A.Assign(I#, 5,  EMENSAGE)
      I# +=1
   END
END
