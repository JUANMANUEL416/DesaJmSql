WHEN 'Accepted'
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
LOC:MGE=''
DISPLAY
GLO:SYS_VISTAPREVIA = 1
IF GLO:CNSCXC='' AND GLO:CNSRPDX=''
   IF LOC:N_FACINICIAL=''
      BEEP(BEEP:SystemHand)
      MESSAGE('Debe Digirar el Nro Factura  de Incio Antes de Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END
   IF LOC:FACTFINAL=''
      BEEP(BEEP:SystemHand)
      MESSAGE('Debe Digirar el Nro Factura  de Final Antes de Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END
   IF LOC:COPIAS<=0
      BEEP(BEEP:SystemHand)
      MESSAGE('Debe Digirar el Nro Copias   Antes de Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END
   IF LOC:FORMATO='' and LOC:FMTOORI=0
      BEEP(BEEP:SystemHand)
      MESSAGE('Debe Digirar Elegir un Formato Antes de Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END

   IF LOC:PREGUNTAR<=0
      BEEP(BEEP:SystemQuestion)
      CASE MESSAGE('Para Evitar inconvenietes con la Impresora y el Sistema logre Terminar el Procceso; Se Sugiere Colocar un Nro para poder Detener la Impresion Si se Requier'&CHR(13)&|
         'Esta Seguro de No Preguntar?','Impresión de Facturas',icon:question,button:yes+button:no, button:no,1)
      OF BUTTON:No
         cycle
      End
   END
   CLEAR(A_FTR:Record)
   A_FTR:N_FACTURA=LOC:N_FACINICIAL
   IF Access:A_FTR.TryFetch(A_FTR:FTRN_FACTURA)
      BEEP(BEEP:SystemHand)
      MESSAGE('No Existe la Factura de Inicial, No se puede Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END
   CLEAR(A_FTR:Record)
   A_FTR:N_FACTURA=LOC:FACTFINAL
   IF Access:A_FTR.TryFetch(A_FTR:FTRN_FACTURA)
      BEEP(BEEP:SystemHand)
      MESSAGE('No Existe la Factura de Final, No se puede Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END
   I#=0 !NRO DE IMPRESIONES
    J#=0 !COPIAS

    LOC:SQL = ' SELECT CNSFCT FROM FTR WHERE N_FACTURA = '&FNSQLFMT(LOC:N_FACINICIAL)
    WDUM{PROP:SQL} = LOC:SQL
    Access:wdum.Next()
    LOC:CNSFCT_INI = WDUM:C1

    LOC:SQL = ' SELECT CNSFCT FROM FTR WHERE N_FACTURA = '&FNSQLFMT(LOC:FACTFINAL)
    WDUM{PROP:SQL} = LOC:SQL
    Access:wdum.Next()
    LOC:CNSFCT_FIN = WDUM:C1


    CLEAR(A_FTR:Record,-1)
    A_FTR:CNSFCT = LOC:CNSFCT_INI
    SET(A_FTR:FTRLLAVE0,A_FTR:FTRLLAVE0)
    LOOP UNTIL Access:A_FTR.Next()
!    CLEAR(A_FTR:Record,-1)
!    A_FTR:N_FACTURA=LOC:N_FACINICIAL
!    SET(A_FTR:FTRN_FACTURA,A_FTR:FTRN_FACTURA)
!    LOOP UNTIL Access:A_FTR.Next()
        IF LOC:DIRIMPRESORA=1
            GLO:NROIMP=3
        ELSE
            GLO:NROIMP=0
        END

        IF A_FTR:CNSFCT < LOC:CNSFCT_INI THEN BREAK.
        IF A_FTR:CNSFCT > LOC:CNSFCT_FIN THEN BREAK.
        IF A_FTR:ESTADO='A' OR A_FTR:CLASEANULACION='NC' THEN CYCLE.
!      IF A_FTR:N_FACTURA<LOC:N_FACINICIAL THEN BREAK.
!      IF A_FTR:N_FACTURA>LOC:FACTFINAL THEN BREAK.
!      IF A_FTR:ESTADO='A' OR A_FTR:CLASEANULACION='NC' THEN CYCLE.

      IF I#=LOC:PREGUNTAR AND LOC:PREGUNTAR>0
         BEEP(BEEP:SystemQuestion)
         CASE MESSAGE('Continuar con la Impresión?','Impresión de Facturas',icon:question,button:yes+button:no, button:no,1)
         OF BUTTON:No
            BREAK
         End
         I#=0
      END
      I#+=1
      GLO:CNSFCT=A_FTR:CNSFCT
      GLO:NOADMISION=A_FTR:NOREFERENCIA
      IF A_FTR:PROCEDENCIA ='SALUD'
         IF LOC:FMTOORI=1 AND A_FTR:IDFORMATO<>''
            LOC:FMT=A_FTR:IDFORMATO
         ELSE
            LOC:FMT=LOC:FORMATOHADM
         END
         Win:ImprimeMasivoFtr(GLO:NOADMISION,A_FTR:N_FACTURA,LOC:FMT,LOC:COPIAS)
         CYCLE
      END
      LOOP J#=1 TO LOC:COPIAS BY 1
         CASE A_FTR:PROCEDENCIA
         OF 'FINANCIERO'
            IF FNVALORVARIABLE('IDTIPOCXP_DEFAULT') = 'ARS'
               CASE fnValorVariable('IDFORMATOFTR')
               OF '01'
                  RptFTR_ARS_1
               OF '02'
                  RptFTR_ARS_2
               else
                  Message('NO ha definido el formato de factura a trabajar')
               end
            ELSIF FNVALORVARIABLE('IDTIPOCXP_DEFAULT') = 'COMER'
               !stop('si entre')
               RPT:FACTURA(A_FTR:CNSFCT)
            ELSE
               IF UPPER(CLIP(FNVALORVARIABLE('IDFORMATOFTRFINANCIE'))) = '02' THEN
                  RPT:FACTURA_02(A_FTR:CNSFCT)
               ELSE
                  if A_FTR:MONEDA <> FnValorVariable('IDMONEDABASE')
                     RPT:FACTURA_MND_EXT
                  ELSE
                     LOC:SQL = 'EXEC SPK_FAC_IMPDEDUC '&FNSQLFMT(DPR:NIT)&','&FNSQLFMT(A_FTR:CNSFCT)&|
                        ','&FNSQLFMT(A_FTR:N_FACTURA)&','&A_FTR:VR_TOTAL
                     WDU5{PROP:SQL} = LOC:SQL
                     IF FNVALORVARIABLE('IDFORMATOFTRFINANCIE')='03'
                        RPT:FACTURAFINAN_03(A_FTR:CNSFCT)
                     ELSIF  FNVALORVARIABLE('IDFORMATOFTRFINANCIE')='04'
                        RPT:FACTURAFINAN_03_TUTEPROV(A_FTR:CNSFCT)
                     ELSE
                        RPT:FACTURA(A_FTR:CNSFCT)
                     END
                  END
               END
            END
         OF 'INVENTARIO'
            RPT:FACTURA_INV(A_FTR:CNSFCT)
         OF 'CE' OROF 'CI'
            CASE A_FTR:TIPOFAC
            OF 'M'
               CASE LOC:FORMATO
               OF '03F'
                  RptFTRCE03F
               OF '03F2'
                  RptFTRCE03F2
               OF '03F3'
                  RptFTRCE03F3
               OF '03GS'
                  RptFTRCE03GS
               OF '03GSG'
                 RptFTRCE03GSanG
               ELSE
                  RptFTRSaludMasiva
               END
            ELSE
               GLO:TIPOCTA = 0
               IF LOC:FMTOORI=1 AND A_FTR:IDFORMATO<>''
                  LOC:FMT=A_FTR:IDFORMATO
               ELSE
                  LOC:FMT=LOC:FORMATO
               END
               CASE LOC:FMT
               OF '01'
                  IF A_FTR:PROCEDENCIA ='CI'
                     IF FNVALORVARIABLE('IDFORMATOFTR_01_CI')='MEDIA'
                        RptFTRCEMedia
                     ELSE
                        RptFTRCE
                     END
                  ELSE
                     IF FNVALORVARIABLE('IDFORMATOFTR_01_CE')='MEDIA'
                        RptFTRCEMedia
                     ELSE
                        RptFTRCE
                     END
                  END
               OF '02'
                !STOP('TIPO2')
                  RptFTRCEnva_
               OF '03B'
                  IF A_FTR:PROCEDENCIA ='CI'
                     RptFTRhadm03bCI('3',A_FTR:IDPLAN, A_FTR:N_FACTURA)
                  ELSE
                     RptFTRhadm03bCE('3',A_FTR:IDPLAN, A_FTR:N_FACTURA)
                  END
               OF '03C'
                  IF A_FTR:PROCEDENCIA ='CI'
                     RptFTRhadm03CI('3',A_FTR:IDPLAN, AUT:N_FACTURA)
                  ELSE
                     RptFTRhadm03CE('3',A_FTR:IDPLAN, AUT:N_FACTURA)
                  END
               of '03CA'
                  IF A_FTR:PROCEDENCIA ='CI'
                     RptFTRhadm03CA_CI('3',A_FTR:IDPLAN, AUT:N_FACTURA)
                  Else
                     RptFTRhadm03CA_CE('3',A_FTR:IDPLAN, AUT:N_FACTURA)
                  End
               of '03F'
                  RptFTRCE03F
               of '03F1'
                  RptFTRCE03F1
               OF '03F2'
                  RptFTRCE03F2
               OF '03GS'
                  RptFTRCE03GS
               OF '03GSG'
                 RptFTRCE03GSanG
                OF '03FIVA'
                    RptFTRCE03FIVA
               OF '04'
                  RptFTRCE1
               of '04A'
                  RptFTRhadm04A_CE
               OF '05'
                  RptFTRCEA
               else
                  Message('NO ha definido el formato de factura a trabajar')
               end
            END
         END
      END
   END
ELSE

   IF LOC:COPIAS<=0
      BEEP(BEEP:SystemHand)
      MESSAGE('Debe Digirar el Nro Copias   Antes de Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END
   IF LOC:FORMATO=''
      BEEP(BEEP:SystemHand)
      MESSAGE('Debe Digirar Elegir un Formato Antes de Comenzar la Impresión','Impresión de Facturas',ICON:Hand)
      CYCLE
   END
   I#=0 !NRO DE IMPRESIONES
   J#=0 !COPIAS
   CLEAR(WDU5:RECORD)
   IF GLO:CNSCXC<>''
      WDU5{PROP:SQL}='SELECT CNSFCT,FCXCD.N_FACTURA FROM FCXCD INNER JOIN FTR ON FCXCD.N_FACTURA=FTR.N_FACTURA WHERE CNSCXC='&FnSQLfmt(GLO:CNSCXC)
   ELSE
      WDU5{PROP:SQL}='SELECT CNSFCT,FTR.N_FACTURA FROM RPDX INNER JOIN FTR ON RPDX.ID1=FTR.N_FACTURA WHERE RPDX.CNS='&FnSQLfmt(GLO:CNSRPDX)
   END
   FREE(QFTRCXC)
   LOOP UNTIL Access:WDU5.Next()
      CNSEFTR=CLIP(WDU5:C1)
      N_FACTURA=CLIP(WDU5:C2)
      ADD(QFTRCXC,+CNSEFTR)
   END
   IF RECORDS(QFTRCXC)>0
      GLO:EXPORTARPDF=1
      GLO:NOMARCHIVO=CLIP(N_FACTURA)
      GLO:PATHPDF=FileLookup6.Ask(0)
      GLO:PATHPDF=GLO:PATHPDF&'\'
   END
   DISABLE(?BComienza)
   SETPATH(LOC:PATH)
   LOOP X#=1 TO RECORDS(QFTRCXC)
      LOC:MGE='Exportando Factura Nro.'&CLIP(N_FACTURA)
      DISPLAY
      GET(QFTRCXC,X#)
      CLEAR(A_FTR:Record)
      A_FTR:CNSFCT=CNSEFTR
      IF Access:A_FTR.TryFetch(A_FTR:FTRLLAVE0) THEN CYCLE.
      IF LOC:DIRIMPRESORA=1
         GLO:NROIMP=3
      ELSE
         GLO:NROIMP=0
      END
      I#+=1
      GLO:CNSFCT=A_FTR:CNSFCT
      GLO:NOADMISION=A_FTR:NOREFERENCIA
      IF A_FTR:PROCEDENCIA ='SALUD'
         IF LOC:FMTOORI=1 AND A_FTR:IDFORMATO<>''
            LOC:FMT=A_FTR:IDFORMATO
         ELSE
            LOC:FMT=LOC:FORMATOHADM
         END
         Win:ImprimeMasivoFtr(GLO:NOADMISION,A_FTR:N_FACTURA,LOC:FMT,LOC:COPIAS)
         CYCLE
      END
      LOOP J#=1 TO LOC:COPIAS BY 1
         CASE A_FTR:PROCEDENCIA
         OF 'FINANCIERO'
            IF FNVALORVARIABLE('IDTIPOCXP_DEFAULT') = 'ARS'
               CASE fnValorVariable('IDFORMATOFTR')
               OF '01'
                  RptFTR_ARS_1
               OF '02'
                  RptFTR_ARS_2
               else
                  Message('NO ha definido el formato de factura a trabajar')
               end
            ELSIF FNVALORVARIABLE('IDTIPOCXP_DEFAULT') = 'COMER'
               !stop('si entre')
               RPT:FACTURA(A_FTR:CNSFCT)
            ELSE
               IF UPPER(CLIP(FNVALORVARIABLE('IDFORMATOFTRFINANCIE'))) = '02' THEN
                  RPT:FACTURA_02(A_FTR:CNSFCT)
               ELSE
                  if A_FTR:MONEDA <> FnValorVariable('IDMONEDABASE')
                     RPT:FACTURA_MND_EXT
                  ELSE
                     LOC:SQL = 'EXEC SPK_FAC_IMPDEDUC '&FNSQLFMT(DPR:NIT)&','&FNSQLFMT(A_FTR:CNSFCT)&|
                        ','&FNSQLFMT(A_FTR:N_FACTURA)&','&A_FTR:VR_TOTAL
                     WDUM{PROP:SQL} = LOC:SQL
                     IF FNVALORVARIABLE('IDFORMATOFTRFINANCIE')='03'
                        RPT:FACTURAFINAN_03(A_FTR:CNSFCT)
                     ELSIF  FNVALORVARIABLE('IDFORMATOFTRFINANCIE')='04'
                        RPT:FACTURAFINAN_03_TUTEPROV(A_FTR:CNSFCT)
                     ELSE
                        RPT:FACTURA(A_FTR:CNSFCT)
                     END
                  END
               END
            END
         OF 'INVENTARIO'
            RPT:FACTURA_INV(A_FTR:CNSFCT)
         OF 'CE' OROF 'CI'
            CASE A_FTR:TIPOFAC
            OF 'M'
               case LOC:FORMATO
               OF '03F'
                  RptFTRCE03F
               OF '03F2'
                  RptFTRCE03F2
               OF '03F3'
                  RptFTRCE03F3
               OF '03GS'
                  RptFTRCE03GS
               OF '03GSG'
                 RptFTRCE03GSanG
               Else
                  RptFTRSaludMasiva
               End
            ELSE
               GLO:TIPOCTA = 0
               IF LOC:FMTOORI=1 AND A_FTR:IDFORMATO<>''
                  LOC:FMT=A_FTR:IDFORMATO
               ELSE
                  LOC:FMT=LOC:FORMATO
               END
               CASE LOC:FMT
               OF '01'
                  IF A_FTR:PROCEDENCIA ='CI'
                     IF FNVALORVARIABLE('IDFORMATOFTR_01_CI')='MEDIA'
                        RptFTRCEMedia
                     ELSE
                        RptFTRCE
                     END
                  ELSE
                     IF FNVALORVARIABLE('IDFORMATOFTR_01_CE')='MEDIA'
                        RptFTRCEMedia
                     ELSE
                        RptFTRCE
                     END
                  END
               OF '02'
                !STOP('TIPO2')
                  RptFTRCEnva_
               OF '03B'
                  IF A_FTR:PROCEDENCIA ='CI'
                     RptFTRhadm03bCI('3',A_FTR:IDPLAN, A_FTR:N_FACTURA)
                  ELSE
                     RptFTRhadm03bCE('3',A_FTR:IDPLAN, A_FTR:N_FACTURA)
                  END
               OF '03C'
                  IF A_FTR:PROCEDENCIA ='CI'
                     RptFTRhadm03CI('3',A_FTR:IDPLAN, AUT:N_FACTURA)
                  ELSE
                     RptFTRhadm03CE('3',A_FTR:IDPLAN, AUT:N_FACTURA)
                  END
               of '03CA'
                  IF A_FTR:PROCEDENCIA ='CI'
                     RptFTRhadm03CA_CI('3',A_FTR:IDPLAN, AUT:N_FACTURA)
                  Else
                     RptFTRhadm03CA_CE('3',FTR:IDPLAN, AUT:N_FACTURA)
                  End
               of '03F'
                  RptFTRCE03F
               of '03F1'
                  RptFTRCE03F1
               OF '03F2'
                  RptFTRCE03F2
               OF '03GS'
                  RptFTRCE03GS
               OF '03GSG'
                 RptFTRCE03GSanG
               OF '04'
                  RptFTRCE1
               of '04A'
                  RptFTRhadm04A_CE
               OF '05'
                  RptFTRCEA
               else
                  Message('NO ha definido el formato de factura a trabajar')
               end
            END
         END
      END
   END
